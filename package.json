{
  "name": "dorper-moda",
  "version": "0.6.0",
  "description": "A system to control stock and sales process to a feshion shop",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist/ && npx tsc",
    "up": "npm run build && sudo docker compose up -d",
    "down": "sudo docker compose down",
    "prepare": "husky install",
    "dev": "ts-node-dev src/main/server.ts",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:ci": "npm test -- --coverage",
    "prettier": "npx prettier --config .prettierrc --write \"src/**/*.ts\""
  },
  "keywords": [
    "Typescript",
    "Nodejs",
    "API",
    "Javascript"
  ],
  "author": "Raniel Sobrinho",
  "license": "MIT",
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^27.4.1",
    "@types/mongodb": "^4.0.7",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.25",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "git-commit-msg-linter": "^4.1.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.8",
    "mockdate": "^3.0.5",
    "morgan": "^1.10.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.2.3"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "express": "^4.18.1",
    "mongodb": "^4.5.0"
  }
}
